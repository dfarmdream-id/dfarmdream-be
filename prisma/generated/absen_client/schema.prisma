generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
  output = "./generated/absen_client"
}

datasource db {
  provider = "postgresql"
  url      = env("ABSEN_DB_URL")
}

model acc_transaction {
  id         String      @id
  app_id      String?
  bio_tbl_id   String?
  company_id   String?
  create_time   DateTime? @db.Timestamp()
  creater_code String?
  creater_id      String?
  creater_name    String?
  op_version Int?
  update_time DateTime? @db.Timestamp()
  updater_code String?
  updater_id String?
  updater_name String?
  acc_zone String?
  acc_zone_code String?
  area_name String?
  capture_photo_path String?
  card_no String?
  dept_code String?
  dept_name String?
  description String?
  dev_alias String?
  dev_id String?
  dev_sn String?
  event_addr Int?
  event_name String?
  event_no Int?
  event_point_id String?
  event_point_name String?
  event_point_type Int?
  event_priority Int?
  event_time DateTime?
  last_name String?
  log_id Int?
  mask_flag String?
  name String?
  pin String?
  reader_name String?
  reader_state Int?
  temperature String?
  trigger_cond Int?
  unique_key String?
  verify_mode_name String?
  verify_mode_no Int?
  vid_linkage_handle String?
}

model pers_personnallist_person {
    id String @id
    app_id String?
    bio_tbl_id String?
    company_id String?
    create_time DateTime?
    creater_code String?
    creater_id String?
    creater_name String?
    op_version Int?
    update_time DateTime?
    updater_code String?
    updater_id String?
    updater_name String?
    email String?
    id_card String?
    link_tbl String?
    mobile_phone String?
    person_birthday DateTime?
    person_gender String?
    person_id String?
    person_name String?
    person_pin String?
    personnallist_id String?
    position_name String?
}